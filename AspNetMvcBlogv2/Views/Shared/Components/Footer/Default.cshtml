@model IEnumerable<AspNetMvcBlogv2.Persistence.Entity.Page>


<footer>
	<section class="footer-2  gray-bg pb-5">

		<div class="footer-btm  pt-4 border-top">
			<div class="row">
				<div class="col-lg-12">
					<ul class="list-inline footer-socials-2 text-center">
						
						@foreach (var apage in Model)
						{
							<li class="list-inline-item"><a asp-controller="Page" asp-action="Detail" asp-route-id ="@apage.Id">@apage.Title</a></li>
						}
					</ul>
				</div>
			</div>
			<div class="row justify-content-center">
				<div class="col-lg-6">
					<div class="copyright text-center ">
						© copyright all reserved to <a href="https://themefisher.com/">Aras Mentese</a>-2023
					</div>
				</div>
			</div>
		</div>

	</section>
</footer>